#!/bin/sh
set -e
if test "$CONFIGURATION" = "Debug"; then :
  cd /Users/cj/Documents/GitHub/chorus/AP2Chorus/build/vst3sdk/public.sdk/samples/vst/note_expression_synth_auv3
  rm -Rf /Users/cj/Documents/GitHub/chorus/AP2Chorus/build/bin/Debug/notexpsynth_auv3_macos_appextension_macos.appex/Contents/PlugIns/plugin.vst3
  /Applications/CMake.app/Contents/bin/cmake -E make_directory /Users/cj/Documents/GitHub/chorus/AP2Chorus/build/bin/Debug/notexpsynth_auv3_macos_appextension_macos.appex/Contents/PlugIns/plugin.vst3
  /bin/cp -a -v /Users/cj/Documents/GitHub/chorus/AP2Chorus/build/VST3/Debug/noteexpressionsynth.vst3/ /Users/cj/Documents/GitHub/chorus/AP2Chorus/build/bin/Debug/notexpsynth_auv3_macos_appextension_macos.appex/Contents/PlugIns/plugin.vst3/
fi
if test "$CONFIGURATION" = "Release"; then :
  cd /Users/cj/Documents/GitHub/chorus/AP2Chorus/build/vst3sdk/public.sdk/samples/vst/note_expression_synth_auv3
  rm -Rf /Users/cj/Documents/GitHub/chorus/AP2Chorus/build/bin/Release/notexpsynth_auv3_macos_appextension_macos.appex/Contents/PlugIns/plugin.vst3
  /Applications/CMake.app/Contents/bin/cmake -E make_directory /Users/cj/Documents/GitHub/chorus/AP2Chorus/build/bin/Release/notexpsynth_auv3_macos_appextension_macos.appex/Contents/PlugIns/plugin.vst3
  /bin/cp -a -v /Users/cj/Documents/GitHub/chorus/AP2Chorus/build/VST3/Release/noteexpressionsynth.vst3/ /Users/cj/Documents/GitHub/chorus/AP2Chorus/build/bin/Release/notexpsynth_auv3_macos_appextension_macos.appex/Contents/PlugIns/plugin.vst3/
fi

